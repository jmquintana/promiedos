{
	"manifest_version": 3,
	"name": "Promiedos",
	"version": "0.0.1",
	"description": "Estad√≠sticas del Torneo de Futbol Argentino",
	"action": {
		"default_title": "Futbol Argentino"
	},
	"content_scripts": [ {
		"js": [ "content.js" ],
		"type": "module",
		"css": [ "style/style.css" ],
		"matches": [ "https://www.promiedos.com.ar/*" ],
		"run_at": "document_end"
	} ],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"permissions": [ "storage", "downloads" ],
	"optional_permissions": [],
	"web_accessible_resources": [ {
		"resources": [
			"app.js",
			"modules/match.model.js",
			"modules/team.model.js",
			"modules/storage.js",
			"modules/utils/utils.js",
			"style/style.css",
			"modules/utils/dom.utils.js",
			"modules/utils/icons/up.svg",
			"modules/utils/icons/equal.svg",
			"modules/utils/icons/down.svg"
		],
		"matches": [ "https://www.promiedos.com.ar/*" ]
	} ]
}